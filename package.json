{
  "name": "vue3-vite-template",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "svgo": "svgo -f src/icons --config=svgo.config.js",
    "lint": "eslint --ext .ts,.tsx,.vue src",
    "ncu": "npx npm-check-updates -u"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "^1.1.0-beta.8",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.7",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.3.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.17.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "sass": "^1.39.0",
    "svgo": "^2.5.0",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-svg-icons": "^1.0.4",
    "vue-tsc": "^0.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
